  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/facturapi/facturapi-node.git"
  },
  "keywords": [
    "cfdi",
    "factura",
    "sat",
    "facturación",
    "facturapi"
  ],
  "author": "Facturación Espacial <contacto@facturapi.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/facturapi/facturapi-node/issues"
