    "form-data": "^4.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/form-data": "^2.5.0",
    "@types/node": "^20.12.12",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.1",
    "lint-staged": "^15.2.4",
    "prettier-eslint": "^16.3.0",
    "prettier-eslint-cli": "^8.0.1",
    "semistandard": "^17.0.0",
